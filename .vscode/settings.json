    },
    "fiveServer.navigate": true,
    "BracketHighlighter.blurOutOfScopeText": true,
    "BracketHighlighter.fontWeight": "bold",
    "BracketHighlighter.highlightScopeFromText": true,
    "BracketHighlighter.textDecoration": "underline",
    "BracketHighlighter.useAngularBrackets": false,
    "BracketHighlighter.blurOpacity": "0.60",
    "css.enabledLanguages": [
        "html"
