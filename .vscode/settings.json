{
    "workbench.iconTheme": "material-icon-theme",
    "files.autoSaveDelay": 100,
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": true,
    "editor.suggestSelection": "first",
    "bracket-pair-colorizer-2.colors": [
        "rgb(250, 69, 69)",
        "rgb(255, 215, 0)",
        "rgb(218, 112, 214)",
        "rgb(135, 206, 250)",
        "rgb(0, 255, 127)",
        "rgb(255, 165, 0)"
    ],
    "bracket-pair-colorizer-2.activeScopeCSS": [
    
        "borderStyle : solid",
        "borderWidth : 1px",
        "borderColor : {color}",
        "opacity: 1"
    ],
    "indentRainbow.colors": [
        
        "rgba(255,255,64,0.07)", 
        "rgba(127,255,127,0.07)",
        "rgba(255,127,255,0.07)",
        "rgba(79,236,236,0.07)",
    ],
    "indentRainbow.updateDelay": 1,
    "color-highlight.languages": [
        "*"
    ],
    
    "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace, 'hack nerd font'",
    "editor.fontLigatures": true,
    "editor.fontWeight": "400",  
    "color-highlight.sass.includePaths": [
    ],
    "color-highlight.matchWords": false,
    "color-highlight.markerType": "dot-before",
    "debug.console.fontFamily": "",
    "editor.codeLensFontFamily": "Consolas, 'Courier New', monospace",
    "settingsSync.ignoredSettings": [
    
    ],
    "color-highlight.enable":   true,
    "liveServer.settings.AdvanceCustomBrowserCmdLine": "",
    "settingsSync.ignoredExtensions": [

    
    ],
    "extensions.autoUpdate": true,
//    "terminal.explorerKind": "external",
    "liveshare.anonymousGuestApproval": "accept",
    "liveServer.settings.wait": 1,
    "workbench.colorTheme": "Palenight Theme",
    "workbench.editorAssociations": {
        "*.png": "cweijan.officeViewer",
        "*.db": "default"
    },
